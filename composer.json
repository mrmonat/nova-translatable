{
    "name": "mrmonat/nova-translatable",
    "description": "A Laravel Nova field for spatie/laravel-translatable.",
    "keywords": [
        "laravel",
        "nova",
        "translatable"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.0|^8.0",
        "spatie/laravel-translatable": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "MrMonat\\Translatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrMonat\\Translatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
